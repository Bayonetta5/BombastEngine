<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SourceDir>$(SolutionDir)..\..\Source\</SourceDir>
    <RootDir>$(SolutionDir)..\..\</RootDir>
    <AssetsDir>$(SolutionDir)..\..\Assets\</AssetsDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="SourceDir">
      <Value>$(SourceDir)</Value>
    </BuildMacro>
    <BuildMacro Include="RootDir">
      <Value>$(RootDir)</Value>
    </BuildMacro>
    <BuildMacro Include="AssetsDir">
      <Value>$(AssetsDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>